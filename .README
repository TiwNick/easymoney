// 3 - Criar a tabela Person com o script abaixo
CREATE TABLE person (
    id NUMBER GENERATED BY DEFAULT AS IDENTITY,
    name varchar(50),
    email varchar(50),
    cpf varchar (20),
    password varchar(50),
    telephone varchar(20),
    dateBirthday DATE,
    balance numeric(18,4) default 0
)
